{"ast":null,"code":"import { themeGet } from \"@styled-system/theme-get\";\nimport { differenceInMinutes } from \"date-fns\";\nimport { ceil } from \"lodash\";\nexport const getTheme = (query, fallback) => themeGet(query, fallback);\nexport const convertHexToRGB = hex => {\n  // check if it's a rgba\n  if (hex.match(\"rgba\")) {\n    let triplet = hex.slice(5).split(\",\").slice(0, -1).join(\",\");\n    return triplet;\n  }\n\n  let c;\n\n  if (/^#([A-Fa-f0-9]{3}){1,2}$/.test(hex)) {\n    c = hex.substring(1).split(\"\");\n\n    if (c.length === 3) {\n      c = [c[0], c[0], c[1], c[1], c[2], c[2]];\n    }\n\n    c = \"0x\" + c.join(\"\");\n    return [c >> 16 & 255, c >> 8 & 255, c & 255].join(\",\");\n  }\n};\nexport const getDateDifference = date => {\n  let diff = differenceInMinutes(new Date(), new Date(date));\n  if (diff < 60) return diff + \" minutes ago\";\n  diff = ceil(diff / 60);\n  if (diff < 24) return `${diff} hour${diff === 0 ? \"\" : \"s\"} ago`;\n  diff = ceil(diff / 24);\n  if (diff < 30) return `${diff} day${diff === 0 ? \"\" : \"s\"} ago`;\n  diff = ceil(diff / 30);\n  if (diff < 12) return `${diff} month${diff === 0 ? \"\" : \"s\"} ago`;\n  diff = diff / 12;\n  return `${diff.toFixed(1)} year${ceil(diff) === 0 ? \"\" : \"s\"} ago`;\n};","map":{"version":3,"sources":["D:/CODE/j_fr_frontend/src/utils/utils.ts"],"names":["themeGet","differenceInMinutes","ceil","getTheme","query","fallback","convertHexToRGB","hex","match","triplet","slice","split","join","c","test","substring","length","getDateDifference","date","diff","Date","toFixed"],"mappings":"AAAA,SAASA,QAAT,QAAyB,0BAAzB;AACA,SAASC,mBAAT,QAAoC,UAApC;AACA,SAASC,IAAT,QAAqB,QAArB;AAEA,OAAO,MAAMC,QAAQ,GAAG,CAACC,KAAD,EAAgBC,QAAhB,KACtBL,QAAQ,CAACI,KAAD,EAAQC,QAAR,CADH;AAGP,OAAO,MAAMC,eAAe,GAAIC,GAAD,IAAS;AACtC;AACA,MAAIA,GAAG,CAACC,KAAJ,CAAU,MAAV,CAAJ,EAAuB;AACrB,QAAIC,OAAO,GAAGF,GAAG,CAACG,KAAJ,CAAU,CAAV,EAAaC,KAAb,CAAmB,GAAnB,EAAwBD,KAAxB,CAA8B,CAA9B,EAAiC,CAAC,CAAlC,EAAqCE,IAArC,CAA0C,GAA1C,CAAd;AACA,WAAOH,OAAP;AACD;;AAED,MAAII,CAAJ;;AAEA,MAAI,2BAA2BC,IAA3B,CAAgCP,GAAhC,CAAJ,EAA0C;AACxCM,IAAAA,CAAC,GAAGN,GAAG,CAACQ,SAAJ,CAAc,CAAd,EAAiBJ,KAAjB,CAAuB,EAAvB,CAAJ;;AACA,QAAIE,CAAC,CAACG,MAAF,KAAa,CAAjB,EAAoB;AAClBH,MAAAA,CAAC,GAAG,CAACA,CAAC,CAAC,CAAD,CAAF,EAAOA,CAAC,CAAC,CAAD,CAAR,EAAaA,CAAC,CAAC,CAAD,CAAd,EAAmBA,CAAC,CAAC,CAAD,CAApB,EAAyBA,CAAC,CAAC,CAAD,CAA1B,EAA+BA,CAAC,CAAC,CAAD,CAAhC,CAAJ;AACD;;AACDA,IAAAA,CAAC,GAAG,OAAOA,CAAC,CAACD,IAAF,CAAO,EAAP,CAAX;AAEA,WAAO,CAAEC,CAAC,IAAI,EAAN,GAAY,GAAb,EAAmBA,CAAC,IAAI,CAAN,GAAW,GAA7B,EAAkCA,CAAC,GAAG,GAAtC,EAA2CD,IAA3C,CAAgD,GAAhD,CAAP;AACD;AACF,CAlBM;AAoBP,OAAO,MAAMK,iBAAiB,GAAIC,IAAD,IAAU;AACzC,MAAIC,IAAI,GAAGlB,mBAAmB,CAAC,IAAImB,IAAJ,EAAD,EAAa,IAAIA,IAAJ,CAASF,IAAT,CAAb,CAA9B;AACA,MAAIC,IAAI,GAAG,EAAX,EAAe,OAAOA,IAAI,GAAG,cAAd;AAEfA,EAAAA,IAAI,GAAGjB,IAAI,CAACiB,IAAI,GAAG,EAAR,CAAX;AACA,MAAIA,IAAI,GAAG,EAAX,EAAe,OAAQ,GAAEA,IAAK,QAAOA,IAAI,KAAK,CAAT,GAAa,EAAb,GAAkB,GAAI,MAA5C;AAEfA,EAAAA,IAAI,GAAGjB,IAAI,CAACiB,IAAI,GAAG,EAAR,CAAX;AACA,MAAIA,IAAI,GAAG,EAAX,EAAe,OAAQ,GAAEA,IAAK,OAAMA,IAAI,KAAK,CAAT,GAAa,EAAb,GAAkB,GAAI,MAA3C;AAEfA,EAAAA,IAAI,GAAGjB,IAAI,CAACiB,IAAI,GAAG,EAAR,CAAX;AACA,MAAIA,IAAI,GAAG,EAAX,EAAe,OAAQ,GAAEA,IAAK,SAAQA,IAAI,KAAK,CAAT,GAAa,EAAb,GAAkB,GAAI,MAA7C;AAEfA,EAAAA,IAAI,GAAGA,IAAI,GAAG,EAAd;AACA,SAAQ,GAAEA,IAAI,CAACE,OAAL,CAAa,CAAb,CAAgB,QAAOnB,IAAI,CAACiB,IAAD,CAAJ,KAAe,CAAf,GAAmB,EAAnB,GAAwB,GAAI,MAA7D;AACD,CAfM","sourcesContent":["import { themeGet } from \"@styled-system/theme-get\";\r\nimport { differenceInMinutes } from \"date-fns\";\r\nimport { ceil } from \"lodash\";\r\n\r\nexport const getTheme = (query: string, fallback?: string) =>\r\n  themeGet(query, fallback);\r\n\r\nexport const convertHexToRGB = (hex) => {\r\n  // check if it's a rgba\r\n  if (hex.match(\"rgba\")) {\r\n    let triplet = hex.slice(5).split(\",\").slice(0, -1).join(\",\");\r\n    return triplet;\r\n  }\r\n\r\n  let c;\r\n\r\n  if (/^#([A-Fa-f0-9]{3}){1,2}$/.test(hex)) {\r\n    c = hex.substring(1).split(\"\");\r\n    if (c.length === 3) {\r\n      c = [c[0], c[0], c[1], c[1], c[2], c[2]];\r\n    }\r\n    c = \"0x\" + c.join(\"\");\r\n\r\n    return [(c >> 16) & 255, (c >> 8) & 255, c & 255].join(\",\");\r\n  }\r\n};\r\n\r\nexport const getDateDifference = (date) => {\r\n  let diff = differenceInMinutes(new Date(), new Date(date));\r\n  if (diff < 60) return diff + \" minutes ago\";\r\n\r\n  diff = ceil(diff / 60);\r\n  if (diff < 24) return `${diff} hour${diff === 0 ? \"\" : \"s\"} ago`;\r\n\r\n  diff = ceil(diff / 24);\r\n  if (diff < 30) return `${diff} day${diff === 0 ? \"\" : \"s\"} ago`;\r\n\r\n  diff = ceil(diff / 30);\r\n  if (diff < 12) return `${diff} month${diff === 0 ? \"\" : \"s\"} ago`;\r\n\r\n  diff = diff / 12;\r\n  return `${diff.toFixed(1)} year${ceil(diff) === 0 ? \"\" : \"s\"} ago`;\r\n};\r\n"]},"metadata":{},"sourceType":"module"}